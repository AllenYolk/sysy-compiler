decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(i32, *i32)

decl @starttime()

decl @stoptime()

fun @main(): i32 {
%entry:
  %and_1 = alloc i32
  %0 = eq 0, 3
  %1 = ne %0, 0
  store %1, %and_1
  br %1, %and_rhs_1, %and_end_1

%and_rhs_1:
  %2 = ne 1, 0
  store %2, %and_1
  jump %and_end_1

%and_end_1:
  %3 = load %and_1
  %4 = sub 1, %3
  ret %4
}
 